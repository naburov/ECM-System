<UserControl x:Class="СЭД_ЭК.EmployeeTab"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:СЭД_ЭК"
             mc:Ignorable="d" 
             d:DesignHeight="1000" d:DesignWidth="960">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="861*"/>
        </Grid.RowDefinitions>
        <ScrollViewer Name="Viewer">
            <DockPanel LastChildFill="True" Grid.Row="1" Name="EmpList"/>
        </ScrollViewer>
    </Grid>
</UserControl>
